{
  "name": "__MSG_AppName__"
, "version": "2023.0.0"
, "short_name": "__MSG_AppShortName__"
, "manifest_version": 3
, "description": "__MSG_AppDesc__"
, "default_locale": "en"
, "icons": {
    "16": "assets/images/icon-16.png"
  , "48": "assets/images/icon-48.png"
  , "128": "assets/images/icon-128.png"
  }
, "options_ui": {
    "page": "options.html"
  , "open_in_tab": false
  }
, "background": {
    "service_worker": "background.js"
  }
, "permissions": [
    "storage"
  , "fontSettings"
  , "webNavigation"
  , "scripting"
  ]
, "host_permissions": [
    "<all_urls>"
  ]
}
